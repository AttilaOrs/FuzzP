adder = @+@ {
	[<-2><-1><0><1><2>]
	[<-2><-1><0><1><2>]
	[<-2><-1><0><1><2>]
	[<-2><-1><0><1><2>]
	[<-2><-1><0><1><2>]
	};
	
sub Cuzz {
	iP0(1.0)-> t0 -> P1 -> t1[1] -> p2(2.0) -> t0[adder];
	p2 <= <1.0>;
	t1-> p3 -> oT4;
}

cOne = new Cuzz;
cTwo = new Cuzz;

iP0(1.0) -> t1 -> cOne.iP0(1.0);
iP1(1.0) -> t1 -> cTwo.iP0(1.0);

cOne.oT4 -> P2 -> oT0;
cTwo.oT4 -> P3 -> oT1;